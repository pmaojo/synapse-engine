<?xml version="1.0"?>
<rdf:RDF xmlns="http://sys.semantic/agriculture#"
     xml:base="http://sys.semantic/agriculture"
     xmlns:core="http://sys.semantic/core#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://sys.semantic/agriculture">
        <owl:imports rdf:resource="http://sys.semantic/core"/>
        
        <!-- External Ontology Imports -->
        <!-- AGROVOC (Agrontology) -->
        <owl:imports rdf:resource="http://aims.fao.org/aos/agrontology"/>
        <!-- Environment Ontology (ENVO) -->
        <owl:imports rdf:resource="http://purl.obolibrary.org/obo/envo.owl"/>
        <!-- FoodOn -->
        <owl:imports rdf:resource="http://purl.obolibrary.org/obo/foodon.owl"/>
        
        <rdfs:comment>Domain ontology for Ecological Agriculture, Permaculture, and Regenerative Farming.</rdfs:comment>
    </owl:Ontology>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    // Classes: Farming Systems & Methodologies
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://sys.semantic/agriculture#FarmingSystem -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#FarmingSystem">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/core#Process"/>
        <rdfs:comment>A systematic approach to agriculture.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Permaculture -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Permaculture">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#FarmingSystem"/>
        <rdfs:comment>Agricultural system based on natural ecosystems.</rdfs:comment>
        <!-- Mapping to AGROVOC 'Permaculture' -->
        <rdfs:seeAlso rdf:resource="http://aims.fao.org/aos/agrovoc/c_35648"/>
        <!-- Mapping to ENVO 'agricultural system' if applicable, or specific biome usage -->
    </owl:Class>

    <!-- http://sys.semantic/agriculture#RegenerativeAgriculture -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#RegenerativeAgriculture">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#FarmingSystem"/>
        <rdfs:comment>Farming principles and practices that increase biodiversity and soil health.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://aims.fao.org/aos/agrovoc/c_92381"/> <!-- AGROVOC: Regenerative Agriculture (approx) -->
    </owl:Class>

    <!-- http://sys.semantic/agriculture#OrganicFarming -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#OrganicFarming">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#FarmingSystem"/>
        <rdfs:label>Agricultura Ecológica</rdfs:label>
        <owl:equivalentClass rdf:resource="http://aims.fao.org/aos/agrovoc/c_11049"/> <!-- AGROVOC: Organic Agriculture -->
    </owl:Class>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    // Classes: Farm Elements & Design
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://sys.semantic/agriculture#AgroEcosystem -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#AgroEcosystem">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/core#Entity"/>
        <!-- Mapping to ENVO 'anthropogenic ecosystem' -->
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000219"/>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#FoodForest -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#FoodForest">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#AgroEcosystem"/>
        <rdfs:label>Bosque Comestible</rdfs:label>
        <rdfs:comment>A low-maintenance, sustainable, plant-based food production and agroforestry system.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#DesignElement -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#DesignElement">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/core#Entity"/>
        <rdfs:comment>Elements used in farm and field design (swales, ponds, beds).</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Swale -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Swale">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#DesignElement"/>
        <rdfs:label>Zanja de Infiltración</rdfs:label>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Guild -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Guild">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#DesignElement"/>
        <rdfs:comment>A group of species that support each other (Permaculture).</rdfs:comment>
    </owl:Class>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    // Classes: Plant Taxonomy (for OWL Reasoning)
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://sys.semantic/agriculture#LivingOrganism -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#LivingOrganism">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/core#Entity"/>
        <rdfs:comment>Any living biological entity.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Plant -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Plant">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#LivingOrganism"/>
        <rdfs:comment>A plant organism.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/FOODON_03411668"/>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Tree -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Tree">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#Plant"/>
        <rdfs:comment>A woody perennial plant.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#FruitTree -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#FruitTree">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#Tree"/>
        <rdfs:comment>A tree that produces edible fruit.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#AppleTree -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#AppleTree">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#FruitTree"/>
        <rdfs:label>Manzano</rdfs:label>
        <rdfs:comment>Malus domestica - Apple tree.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Vegetable -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Vegetable">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#Plant"/>
        <rdfs:comment>An edible plant or part of a plant.</rdfs:comment>
    </owl:Class>

    <!-- http://sys.semantic/agriculture#Herb -->
    <owl:Class rdf:about="http://sys.semantic/agriculture#Herb">
        <rdfs:subClassOf rdf:resource="http://sys.semantic/agriculture#Plant"/>
        <rdfs:comment>A plant valued for flavor, scent, or medicinal properties.</rdfs:comment>
    </owl:Class>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    // Properties
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://sys.semantic/agriculture#usesMethod -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#usesMethod">
        <rdfs:domain rdf:resource="http://sys.semantic/agriculture#AgroEcosystem"/>
        <rdfs:range rdf:resource="http://sys.semantic/agriculture#FarmingSystem"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#hasDesignElement">
        <rdfs:domain rdf:resource="http://sys.semantic/agriculture#AgroEcosystem"/>
        <rdfs:range rdf:resource="http://sys.semantic/agriculture#DesignElement"/>
    </owl:ObjectProperty>

    <!-- http://sys.semantic/agriculture#improves -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#improves">
        <rdfs:comment>Indicates that one entity improves the condition or quality of another.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- http://sys.semantic/agriculture#partOf -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#partOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>Indicates that one entity is part of another (transitive).</rdfs:comment>
    </owl:ObjectProperty>

    <!-- http://sys.semantic/agriculture#contains -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#contains">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:inverseOf rdf:resource="http://sys.semantic/agriculture#partOf"/>
        <rdfs:comment>Indicates that one entity contains another (transitive, inverse of partOf).</rdfs:comment>
    </owl:ObjectProperty>

    <!-- http://sys.semantic/agriculture#growsIn -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#growsIn">
        <rdfs:domain rdf:resource="http://sys.semantic/agriculture#Plant"/>
        <rdfs:range rdf:resource="http://sys.semantic/agriculture#AgroEcosystem"/>
        <rdfs:comment>Indicates where a plant grows.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- http://sys.semantic/agriculture#supports -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#supports">
        <owl:inverseOf rdf:resource="http://sys.semantic/agriculture#growsIn"/>
        <rdfs:domain rdf:resource="http://sys.semantic/agriculture#AgroEcosystem"/>
        <rdfs:range rdf:resource="http://sys.semantic/agriculture#Plant"/>
        <rdfs:comment>Indicates that an ecosystem supports a plant (inverse of growsIn).</rdfs:comment>
    </owl:ObjectProperty>

    <!-- http://sys.semantic/agriculture#fixes -->
    <owl:ObjectProperty rdf:about="http://sys.semantic/agriculture#fixes">
        <rdfs:domain rdf:resource="http://sys.semantic/agriculture#Plant"/>
        <rdfs:comment>Indicates that a plant fixes a substance (e.g., nitrogen).</rdfs:comment>
    </owl:ObjectProperty>

</rdf:RDF>
